<p id="notice"><%= notice %></p>

<h1>Bookings</h1>

<table>
  <thead>
    <tr>
      <th>Customer</th>
      <th>Cleaner</th>
      <th>Date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bookings.each do |booking| %>
      <tr>
        <td><%= booking.customer_name%></td>
        <td><%= booking.cleaner_name%></td>
        <td><%= booking.date %></td>
      </tr>
    <% end %>
  </tbody>
</table>
