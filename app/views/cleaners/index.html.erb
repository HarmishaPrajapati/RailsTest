<p id="notice"><%= notice %></p>

<h1>Cleaners</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Email</th>
      <th>City</th>
      <th>Quality score</th>
      <th>Booking</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cleaners.each do |cleaner| %>
      <tr>
        <td><%= cleaner.first_name %></td>
        <td><%= cleaner.last_name %></td>
        <td><%= cleaner.email %></td>
        <td><%= cleaner.city %></td>
        <td><%= cleaner.quality_score %></td>
        <td><%= link_to 'Book Cleaner',  new_booking_path  %></td>
      </tr>
    <% end %>
  </tbody>
</table>
